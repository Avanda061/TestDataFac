{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "TestDataFacADO"
		},
		"AzureFileStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureFileStorage1'"
		},
		"Sftp1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Sftp1'"
		},
		"Nikko_SBIF_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": " https://www.nikkoam.com.sg/files/documents/abf_singapore_bond_index_fund/"
		},
		"Sftp1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "sftp.bloomberg.com"
		},
		"Sftp1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "t"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "TestDataFacADO_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureFileStorage1_connectionString": {
						"value": "[parameters('AzureFileStorage1_connectionString')]"
					},
					"Sftp1_password": {
						"value": "[parameters('Sftp1_password')]"
					},
					"Nikko_SBIF_properties_typeProperties_url": {
						"value": "[parameters('Nikko_SBIF_properties_typeProperties_url')]"
					},
					"Sftp1_properties_typeProperties_host": {
						"value": "[parameters('Sftp1_properties_typeProperties_host')]"
					},
					"Sftp1_properties_typeProperties_userName": {
						"value": "[parameters('Sftp1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}